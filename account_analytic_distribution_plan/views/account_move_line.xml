<?xml version="1.0"?>
<openerp>
    <data>

        <record id="view_move_form" model="ir.ui.view">
            <field name="name">account.move.form.inherit2</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account_analytic_distribution.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_distribution_ids']"
                       position="before">
                    <field name="distribution_plan_id" />
                </xpath>
            </field>
        </record>

        <record id="view_move_line_form" model="ir.ui.view">
            <field name="name">account.move.line.form.inherit2</field>
            <field name="model">account.move.line</field>
            <field name="inherit_id" ref="account_analytic_distribution.view_move_line_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_distribution_ids']"
                       position="before">
                    <field name="distribution_plan_id" />
                </xpath>
            </field>
        </record>

        <record id="view_move_line_form2" model="ir.ui.view">
            <field name="name">account.move.line.form.inherit2</field>
            <field name="model">account.move.line</field>
            <field name="priority">2</field>
            <field name="inherit_id" ref="account_analytic_distribution.view_move_line_form2"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_distribution_ids']"
                       position="before">
                    <field name="distribution_plan_id" />
                </xpath>
            </field>
        </record>

        <record id="view_move_line_tree" model="ir.ui.view">
            <field name="name">account.move.line.tree.inherit2</field>
            <field name="model">account.move.line</field>
            <field name="priority">2</field>
            <field name="inherit_id" ref="account_analytic_distribution.view_move_line_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_distribution_ids']"
                       position="before">
                    <field name="distribution_plan_id" />
                </xpath>
            </field>
        </record>

    </data>
</openerp>
